<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Sherullah Il Moazzam 1444H</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
        crossorigin="anonymous"></script>


    <script type="text/javascript">
        function EnableDisableButton(books) {
            var borrow = document.getElementById("borrow");
            borrow.disabled = books.checked ? false : true;
        }

    </script>

    <script>
        function EnableDisableButtonBorrow(books_borrowed) {
            var return_btn = document.getElementById("return");
            return_btn.disabled = books_borrowed.checked ? false : true;
        }
    </script>

</head>

<body>
    {{#each user}}
   

       <div id="card" class=" w-50 m-2 border-2 "  >
    <div class=" form d-flex flex-column justify-content-center align-items-center bg-primary p-3 text-white">
      <label for="name" class="fw-bold fs-3">DAWOODI BOHRA JAMAAT </label>
      <label for="namr">NOORANI NAGAR,INDORE</label>
      <label for="namr" class="fs-4">AZAAN/TAKBIRAH  Raza PASS</label>

    </div>
    <div class=" mt-4 flex-column" >
      <div class="d-flex">
        <label for="name" style="width:23%">full Name</label>
        <p class="form-control" id="name"> {{this.name}}</p>
      </div>
      <div class="d-flex">

        <label for="name" style="width:23%">ITS ID</label>
        <p class="form-control" id="its">{{this.its}}</p>
      </div>
      <div class="d-flex">
        <label for="name" style="width:23%">Mobile No:</label>
        <p class="form-control" id="mobile">{{this.mobile}}</p>
      </div>
      <div class="d-flex">

        <label for="name" style="width:23%">Sabeel Code</label>
        <p class="form-control" id="sabeel">{{this.sabeel}}</p>
      </div>
      
    
    </div>
      <div class="d-flex bg-success justify-content-center">

        <p class="fs-1 text-white" id="event">{{this.event}}</p>
      </div>


  </div>
   {{/each}}
   <div>
    <button class="btn btn-success" onclick="window.print()">print</button>
   </div>

</body>

</html>